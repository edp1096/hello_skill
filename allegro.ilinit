/*
원래 reload 기능은 없었는데, 필요해서 추가함.
주석내 코드는 처음 작업한 원본.
dir = getWorkingDir()
foreach(file rexMatchList("\\.il$" getDirFiles(dir))
	printf("Loading Skill file: %s\n" file)
	load(file)
)
*/

axlCmdRegister("11" `loadSkillFiles)

procedure( loadSkillFiles()
	dir = getWorkingDir()
	foreach(file rexMatchList("\\.il$" getDirFiles(dir))
		printf("Loading Skill file: %s\n" file)
		load(file)
	)
)

loadSkillFiles
